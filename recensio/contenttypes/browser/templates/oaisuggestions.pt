<div id="oaisuggestions" i18n:domain="recensio" tal:condition="view/show">
    <div style="display:none;" class="oaisuggestion" 
        tal:attributes="id python: ignore==1 and 'oaisuggestion_template' or ''" tal:repeat="ignore python: [1,2]">
        OAI Suggestion
        <ul>
            <li style="border-style:solid; border-width: 1px; border-collapse:collapse;display:block;" class="dc_definition"><def style="display:inline; padding-right: 10px;">Some example key</def><ul class="dc_values" style="display:inline-block"><li style="display:block; background-color:#eee;">Some example value</li></ul></li>
        </ul>
        <form>
            <input type="button" name="use_data" value="use this data" i18n:attributes="value button_use_this_data" />
        </form>
    </div>
</div>
